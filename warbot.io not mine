var g_lang = "";
var g_moreio = "inline";

if (navigator.appName=="Netscape")        
     g_lang = navigator.language;
else g_lang = navigator.userLanguage;
g_lang = g_lang.substr(0,2);

  // adinplay init
  window.aiptag = window.aiptag || {cmd: []};
  aiptag.cmd.display = aiptag.cmd.display || [];
  aiptag.cmd.player = aiptag.cmd.player || [];
  //CMP tool settings
  aiptag.cmp = {
    show: true,
    position: "centered",  //centered, bottom
    button: true,
    buttonText: "Privacy settings",
    buttonPosition: "bottom-left" //bottom-left, bottom-right, top-left, top-right
  }

  aiptag.cmd.player.push(function() {
    adplayer = new aipPlayer({
      AD_WIDTH: 960,
      AD_HEIGHT: 540,
      AD_FULLSCREEN: true,
      AD_CENTERPLAYER: false,
      LOADING_TEXT: 'loading advertisement',
      PREROLL_ELEM: function(){return document.getElementById('preroll')},
      AIP_COMPLETE: function ()  { gameInstance.SendMessage('GameMain', 'ShowAdsComplete'); },
      AIP_REMOVE: function ()  {}
    });
  });


function updatesize() {
  var w = document.body.clientWidth;
  var h = document.body.clientHeight;
  var w0 = 960/2;
  var h0 = 600/2;
  var h1 = parseInt(w * h0 / w0);
  var w2 = parseInt(h * w0 / h0);
  var rate_disp = 'none';  
  var container = document.getElementById("gameContainer")
  if (h1 <= h && w > w0){
    container.style.width = w + 'px';
    container.style.height = h1 + 'px';
    rate_disp = 'inline';
  }
  else if (h > h0 && w2 <= w){
    container.style.width = w2 + 'px';
    container.style.height = h + 'px';
  }
  else{
    container.style.width = w0 + 'px';
    container.style.height = h0 + 'px';
  }
  if ( g_lang != "ko" )
    rate_disp = 'none';  

  var rateinfo = document.getElementById("rateinfo");
  if (rateinfo)
    rateinfo.style.display = rate_disp;

  var moreio = document.getElementById("moreio");
  if (moreio)
    moreio.style.display = g_moreio;
  
  var canvas = document.getElementById("#canvas");
  if (canvas != null){
    canvas.style.width = container.style.width;
    canvas.style.height = container.style.height;
  } 
}



function popup(url, winName, windWidth, winHeight){
  var x = (screen.width - windWidth) / 2;
  var y = (screen.height - winHeight) / 2;
  return window.open(url, winName, "toolbar=0,menubar=0,scrollbars=no,resizable=no,left="+ x +",top="+ y +",width="+ windWidth +",height="+ winHeight);
}

function checkUrlParams() {
    var params = {};
    window.location.search.replace(/[?&]+([^=&]+)=([^&]*)/gi, function(str, key, value) { params[key] = value; });
    if (params.language)
      g_moreio = "none";
} 

var spinner = null; 

function UnityInitialization(){
//console.log("GameInitialization");
  var loading = document.getElementById("loading");
  if (loading)
      loading.style.display = 'none';

  if (spinner) spinner.stop();
}

function UnityProgress(gameInstance, progress) {
  if (!gameInstance.Module)
    return;
  //console.log("progress="+ progress); // 0 ~ 1
  if ( !spinner ) {
    var target  = document.getElementById('spinicon');
    if ( target ) {
      spinner = new Spinner().spin();
      target.appendChild(spinner.el);
    }    
  }

}

var advisible = 0;
var state = '';
function UnityEvent(evt) {
  if (state == "GameResult" && advisible == 1)
  {
    document.getElementById('warbot-io_300x250').style.display = 'none';
    advisible = -1;
  }
  if (evt == "GameResult")
  {
    if (advisible==0)
      aiptag.cmd.display.push(function() { aipDisplayTag.display('warbot-io_300x250'); });
    else
      aiptag.cmd.display.push(function() { aipDisplayTag.refresh('warbot-io_300x250'); });
    advisible = 1;
    document.getElementById('warbot-io_300x250').style.display = 'block';
    console.log("aipDisplayTag.display");
  }
  if (evt == "ShowAds")
  {
    if (typeof adplayer !== 'undefined' ){
      aiptag.cmd.player.push(function() { adplayer.startPreRoll(); });
    } else {
      console.log("Ad Could not be loaded, please turn-off your adblockers!");
    }
  }
  state = evt;
  console.log("Event:" + evt);
}

  checkUrlParams();

  var gameInstance = UnityLoader.instantiate("gameContainer", "Build122/webgl.json", {onProgress: UnityProgress,
    Module: { TOTAL_MEMORY: 568435456, onRuntimeInitialized: UnityInitialization, securemode: window.location.protocol == 'https:' ? 1:0 }});


</script>

